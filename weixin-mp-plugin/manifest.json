{
	"manifest_version": 2,
	"name":"微信公众号助手",
	"version": "1.0.0",
	"description": "微信公众号助手",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "微信公众号助手"
	},
	"permissions": [
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	]
}